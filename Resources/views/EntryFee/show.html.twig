{% extends '@Design/Default/formbase.html.twig' %}

{% block form -%}
    <h2>EntryFees</h2>
    <a href="{{ path('entryfee') }}" class="btn-sm btn-primary">
        Back
    </a>
    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Entryfee</th>
            <td>{{ entity.entryFee }}</td>
        </tr>
        <tr>
            <th>Entryfeestring</th>
            <td>{{ entity.entryFeeString }}</td>
        </tr>
        <tr>
            <td>
                <a href="{{ path('entryfee_edit', { 'id': entity.id }) }}" class="btn-sm btn-info">
                    Edit
                </a>
                {{ form(delete_form) }}
            </td>
        </tr>
        </tbody>
    </table>


{% endblock %}
