{% extends '@Design/Default/formbase.html.twig' %}

{% block form -%}
    <h2>ContestStyle</h2>

    <table class="record_properties">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('admin_ContestStyle') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('admin_ContestStyle_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock %}
